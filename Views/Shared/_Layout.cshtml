<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title Policy Checker</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

   

</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
        <a class="navbar-brand" href="#"> Paragon Policy Checker</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarColor01">
            <ul class="navbar-nav mr-auto">
                <li class="nav-link">
                    @Html.ActionLink("Home", "Index", "Home", new object { },
new { @class = "navWhite" })
                </li>
                <li class="nav-link">
                    @Html.ActionLink("Add Policy", "Create", "Home", new object { },
new { @class = "navWhite" })
                </li>
                <li class="nav-link">
                    @Html.ActionLink("Upload Policy", "UploadPolicy", "Home", new object { },
new { @class = "navWhite" })
                </li>
                <li class="nav-link">
                    @Html.ActionLink("Upload Quote", "UploadQuote", "Home", new object { },
new { @class = "navWhite" })
                </li>

            </ul>

            <form class="form-inline my-2 my-lg-0">
                <div class="nav-link navLogin">@Html.Partial("_LoginPartial")</div>
            </form>
        </div>
    </nav>


    <div class="container body-content">
        @RenderBody()

        <footer></footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="https://unpkg.com/ionicons@4.3.0/dist/ionicons.js"></script>
</body>
</html>
