@model PolicyCheck.Models.CAllrisksBicycle
@using PolicyCheck.Helper


@Styles.Render("~/Content/css")
@Scripts.Render("~/bundles/modernizr")

<div class="editorRow">


    @using (Html.BeginCollectionItem("Bicycles"))
    {
        <div class="form-group whiteDiv">


            <div id="allrisksTitle2" class="wrapper8">
                <div class="form-group" style="float: left; width: 150px; margin-right:30px">
                    <p class=" = font-weight-bold popOut">Value</p>
                </div>
                <div class="form-group" style="float: left; width: 150px; margin-right:30px ">
                    <p class=" = font-weight-bold popOut">Make</p>
                </div>
                <div class="form-group" style="float: left; width: 150px; margin-right:30px">
                    <p class=" = font-weight-bold popOut">Model</p>
                </div>

                <div class="form-group" style="float: left; width: 150px; margin-right:30px ">
                    <p class=" = font-weight-bold popOut">Territory</p>
                </div>


                <div class="wrapper8">
                    <div class="form-group" style="float: left; width: 150px; margin-right:30px; ">
                        @Html.TextBoxFor(x => x.BicycleValue, htmlAttributes: new { @class = "form-control" })
                    </div>

                    <div class="form-group" style="float: left; width: 150px; margin-right:30px ">
                        @Html.TextBoxFor(x => x.BicycleMake, new { @class = "form-control" })
                    </div>
                    <div class="form-group" style="float: left; width: 150px; margin-right:30px ">
                        @Html.TextBoxFor(x => x.BicycleModel, new { @class = "form-control" })
                    </div>

                    <div class="form-group" style="float: left; width: 150px; margin-right:30px ">
                        @Html.DropDownListFor(x => x.BicycleTerritory, Model.BicycleTerritory2, "MS", new { @id = "buildingsContent", @class = "form-control" })
                       
                    </div>


                </div>
            </div>
        </div>





        <br />




        @*<a href="#" class="deleteRow btn btn-primary">delete</a>*@
    }
</div>

@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")

@section Scripts
{

}
